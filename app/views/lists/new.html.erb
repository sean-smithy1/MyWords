<% provide(:title, 'New List') %>
<h1>Add List</h1>

    <%= form_for(@list) do |list| %>
    <%= render 'shared/validation_errors', object: @list %>

      <%= list.label :listname, 'List Name' %>
      <%= list.text_field :listname %>

      <div class="btn-group-sm">
        <%= list.submit "Create List", class: "btn btn-primary" %>
      </div>

    <% end %>

