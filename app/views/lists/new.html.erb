<% provide(:title, 'New List') %>
<h1>Add List</h1>

    <%= form_for(@list) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :listname, 'List Name' %>
      <%= f.text_field :listname %>

      <%= f.submit "Create List", class: "btn btn-primary" %>
    <% end %>

